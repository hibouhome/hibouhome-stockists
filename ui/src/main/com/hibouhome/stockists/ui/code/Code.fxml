<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.hibouhome.stockists.ui.code.CodeController">
	<children>
		<Button mnemonicParsing="true" onAction="#copyToClipboard" text="_Copy to Clipboard" VBox.vgrow="NEVER" />
		<TextArea fx:id="textArea" editable="false" wrapText="true" VBox.vgrow="ALWAYS" />
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</VBox>
